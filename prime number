n=int(input("enter the number="))
count=0

for i in range(1,n+1):
    if(n%i==0):
            count=count+1
if(count==2):
        print("given number is prime number")
elif(count==1):
        print("given number is neither prime nor composite")    
else:
        print("given number is not a prime number")
        
